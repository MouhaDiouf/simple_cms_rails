<h1>All Users</h1>
<%= link_to("Create User", new_admin_user_path) %>
<div class="users index">
<table class="listing" summary="Users listing">
  <tr>
  <th>Username</th>
  <th>First Name</th>
  <th>Last Name</th>
  <th>Email</th>
  <th>Actions</th>
  </tr>
  <% @users.each do |user| %>
  <tr>
    <td><%= user.username %></td>
    <td><%= user.first_name %></td>
    <td><%= user.last_name %></td>
    <td><%= user.email %></td>
    <td>
      <%=link_to("Edit", edit_admin_user_path(user)) %>
      <%= link_to('Destroy', delete_admin_user_path(user))%>
    </td>
  </tr>
<% end %>
</table>
</div>
